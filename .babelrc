{
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        [
            "@babel/plugin-transform-typescript",
            {
                "isTSX": true
            }
        ],
        [
            "module-resolver",
            {
                "root": ["./lib"],
                "alias": {
                    "@core": "./core",
                    "@utils": "./utils"
                }
            }
        ]
    ],
    "presets": ["@babel/typescript", "@babel/preset-react"]
}
